{
  "expo": {
    "name": "CAMO-Admin",
    "slug": "camorent-admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-black.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/icon-black.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.camorent.admin",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-black.png",
        "backgroundColor": "#000000"
      },
      "package": "com.camorent.admin",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.WAKE_LOCK",
        "android.permission.USE_FULL_SCREEN_INTENT",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.ACCESS_NOTIFICATION_POLICY"
      ],
      "useNextNotificationsApi": true,
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/icon-black.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon-black.png",
          "color": "#000000",
          "defaultChannel": "booking_requests",
          "sounds": ["./assets/notification_sound.wav"],
          "mode": "production",
          "androidMode": "default",
          "androidCollapsedTitle": "ðŸŽ¬ New Booking Request"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "be247bc3-bf74-4c3a-a99e-4e7e945f4e84"
      }
    },
    "owner": "hemant-camo"
  }
}
